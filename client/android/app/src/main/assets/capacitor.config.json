{
  "appId": "io.ionic.starter",
  "appName": "client2",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "build",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "CapacitorFirebaseAuth": {
      "providers": ["google.com", "twitter.com", "facebook.com", "phone"],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": ["profile", "https://www.googleapis.com/auth/drive"]
      }
    }
  },
  "cordova": {}
}
